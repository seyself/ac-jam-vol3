{
  "env": {
    "node"   : true,
    "browser": true,
    "es6"    : true,
    "jquery" : true
  },
  "extends": "eslint:recommended",
  "installedESLint": true,
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "globals": {
    "assert": false,
    "it": false,
    "afterEach": false,
    "beforeEach": false,
    "describe": false
  },
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}